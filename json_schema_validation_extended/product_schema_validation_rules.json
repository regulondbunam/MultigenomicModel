{
	"validator": {
		"$jsonSchema": {
			"bsonType": "object",
			"required": [
				"_id",
				"organism_id",
				"gene_id",
				"schemaVersion"
			],
			"oneOf": [{
					"required": ["anticodon"],
					"not": {
						"required": ["molecularWeight", "molecularWeightKd", "catalyzes"]
					}
				},
				{
					"not": {
						"required": ["anticodon"]
					}
				}
			],
			"properties": {
				"_id": {
					"bsonType": "string"
				},
				"name": {
					"bsonType": "string"
				},
				"isoelectricPoint": {
					"bsonType": "double"
				},
				"location": {
					"bsonType": "string"
				},
				"sequence": {
					"bsonType": "string"
				},
				"note": {
					"bsonType": "string"
				},
				"type": {
					"bsonType": "string"
				},
				"organism_id": {
					"bsonType": "string"
				},
				"modifiedForm": {
					"bsonType": "string"
				},
				"anticodon": {
					"bsonType": "string"
				},
				"molecularWeight": {
					"bsonType": "double"
				},
				"molecularWeightKd": {
					"bsonType": "double"
				},
				"catalyzes": {
					"bsonType": "string"
				},
				"componentOf": {
					"bsonType": "string"
				},
				"codingSegments": {
					"bsonType": "array",
					"uniqueItems": "true",
					"items": {
						"bsonType": "int"
					}
				},
				"spliceFormIntrons": {
					"bsonType": "array",
					"uniqueItems": "true",
					"items": {
						"bsonType": "int"
					}
				},
				"productFunction": {
					"bsonType": "string",
					"enum": [
						"transcriptionFactor",
						"srna",
						"ppgpp"
					]
				},
				"siteLength": {
					"bsonType": "int"
				},
				"consensusSequence": {
					"bsonType": "string"
				},
				"symmetry": {
					"bsonType": "string"
				},
				"gene_id": {
					"bsonType": "string"
				},
				"synonyms": {
					"bsonType": "array",
					"uniqueItems": true,
					"items": {
						"bsonType": "string"
					}
				},
				"evidenceReferences": {
					"bsonType": "array",
					"uniqueItems": true,
					"items": {
						"bsonType": "object",
						"anyOf": [{
							"required": ["publication_id"]
						}, {
							"required": ["evidence_id"]
						}],
						"properties": {
							"publication_id": {
								"bsonType": "string"
							},
							"evidence_id": {
								"bsonType": "string"
							}
						},
						"additionalProperties": false
					}
				},
				"externalCrossReferences": {
					"bsonType": "array",
					"uniqueItems": true,
					"items": {
						"bsonType": "object",
						"required": [
							"externalCrossReference_id",
							"objectId"
						],
						"properties": {
							"externalCrossReference_id": {
								"bsonType": "string"
							},
							"objectId": {
								"bsonType": "string"
							}
						},
						"additionalProperties": false
					}
				},
				"internalComment": {
					"bsonType": "string"
				},
				"schemaVersion": {
					"bsonType": "double"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}