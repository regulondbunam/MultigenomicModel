									"type": "object",
									"properties": {
										"term_id": {
											"type": "string",
											"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
										},
										"term_name": {
											"type": "string"
										}
									},
									"additionalProperties": false
								}
							},
							"molecularFunction": {
								"type": "array",
								"uniqueItems": true,
								"items": {
									"type": "object",
									"properties": {
										"term_id": {
											"type": "string",
											"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
										},
										"term_name": {
											"type": "string"
										}
									},
									"additionalProperties": false
