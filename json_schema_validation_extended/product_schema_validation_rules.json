{
	"validator": {
		"$jsonSchema": {
			"type": "object",
			"required": ["_id", "organism_id", "gene_id"],
			"oneOf": [{
				"required": ["anticodon"],
				"not": {
					"anyOf": [{
						"required": ["molecularWeight"]
					}, {
						"required": ["molecularWeightKd"]
					}, {
						"required": ["catalyzes"]
					}]
				}
			}, {
				"not": {
					"anyOf": [{
						"required": ["anticodon"]
					}]
				}
			}],
			"properties": {
				"_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"name": {
					"type": "string"
				},
				"isoelectricPoint": {
					"type": "number"
				},
				"location": {
					"type": "string"
				},
				"sequence": {
					"type": "string"
				},
				"note": {
					"type": "string"
				},
				"type": {
					"type": "string"
				},
				"modifiedForm": {
					"type": "string"
				},
				"anticodon": {
					"type": "string"
				},
				"molecularWeight": {
					"type": "number"
				},
				"molecularWeightKd": {
					"type": "number"
				},
				"catalyzes": {
					"type": "string"
				},
				"componentOf": {
					"type": "string"
				},
				"codingSegments": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "number",
						"multipleOf": 1.0
					}
				},
				"spliceFormIntrons": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "number",
						"multipleOf": 1.0
					}
				},
				"productType": {
					"type": "string",
					"enum": ["transcriptionFactor", "srna", "ppgpp"]
				},
				"siteLength": {
					"type": "number",
					"multipleOf": 1.0
				},
				"consensusSequence": {
					"type": "string"
				},
				"symmetry": {
					"type": "string"
				},
				"gene_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"term_ids": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "string",
						"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
					}
				},
				"synonyms": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "string"
					}
				},
				"evidenceReferences": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "object",
						"anyOf": [{
							"required": ["publication_id"]
						}, {
							"required": ["evidence_id"]
						}],
						"properties": {
							"publication_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							},
							"evidence_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							}
						},
						"additionalProperties": false
					}
				},
				"externalCrossReferences": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "object",
						"required": ["externalCrossReference_id", "objectId"],
						"properties": {
							"externalCrossReference_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							},
							"objectId": {
								"type": "string"
							}
						},
						"additionalProperties": false
					}
				},
				"organism_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"internalComment": {
					"type": "string"
				},
				"schemaVersion": {
					"type": "number"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}