						"type": "object",
						"anyOf": [{
							"required": ["publication_id"]
						}, {
							"required": ["evidence_id"]
						}],
						"properties": {
							"publication_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							},
							"evidence_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							}
