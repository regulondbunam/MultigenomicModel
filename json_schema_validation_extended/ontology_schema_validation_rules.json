{
	"validator": {
		"$jsonSchema": {
			"type": "object",
			"required": [
				"_id",
				"name",
				"type",
				"url"
			],
			"properties": {
				"_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"name": {
					"type": "string"
				},
				"type": {
					"type": "string",
					"enum": ["external", "local"]
				},
				"url": {
					"type": "string"
				},
				"description": {
					"type": "string"
				},
				"applyTo": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "string"
					}
				},
				"label": {
					"type": "string"
				},
				"schemaVersion": {
					"type": "number"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}