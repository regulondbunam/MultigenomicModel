								"items": {
									"bsonType": "array"
								}
							},
							"note": {
								"bsonType": "string"
							}
						}
					}
				},
				"type": {
					"bsonType": "string"
				},
				"creationDate": {
					"bsonType": "date"
				},
				"updateHistory": {
					"bsonType": "array",
					"uniqueItems": true,
					"items": {
						"bsonType": "date"
					}
				},
				"schemaVersion": {
					"bsonType": "double"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}