{
	"validator": {
		"$jsonSchema": {
			"type": "object",
			"required": [
				"_id",
				"name",
				"ontology_id"
			],
			"properties": {
				"_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"definition": {
					"type": "string"
				},
				"evidenceReferences": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "object",
						"anyOf": [{
							"required": ["publication_id"]
						}, {
							"required": ["evidence_id"]
						}],
						"properties": {
							"publication_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							},
							"evidence_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							}
						},
						"additionalProperties": false
					}
				},
				"externalCrossReferences": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "object",
						"required": [
							"externalCrossReference_id",
							"objectId"
						],
						"properties": {
							"externalCrossReference_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							},
							"objectId": {
								"type": "string"
							}
						},
						"additionalProperties": false
					}
				},
				"has": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "string"
					}
				},
				"isA": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "string"
					}
				},
				"members": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "string"
					}
				},
				"name": {
					"type": "string"
				},
				"ontology_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"synonyms": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "string"
					}
				},
				"url": {
					"type": "string"
				},
				"schemaVersion": {
					"type": "number"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}