{
	"validator": {
		"$jsonSchema": {
			"bsonType": "object",
			"required": [
				"_id",
				"name",
				"schemaVersion"
			],
			"properties": {
				"_id": {
					"bsonType": "string"
				},
				"name": {
					"bsonType": "string"
				},
				"description": {
					"bsonType": "string"
				},
				"url": {
					"bsonType": "string"
				},
				"note": {
					"bsonType": "string"
				},
				"internalComment": {
					"bsonType": "string"
				},
				"schemaVersion": {
					"bsonType": "double"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}