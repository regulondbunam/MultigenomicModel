{
	"validator": {
		"$jsonSchema": {
			"type": "object",
			"required": [
				"_id",
				"distanceToGene",
				"gene_id",
				"organism_id"
			],
			"properties": {
				"_id": {
					"type": "string"
				},
				"leftEndPosition": {
					"type": "number",
					"multipleOf": 1.0
				},
				"rightEndPosition": {
					"type": "number",
					"multipleOf": 1.0
				},
				"sequence": {
					"type": "string"
				},
				"note": {
					"type": "string"
				},
				"distanceToGene": {
					"type": "number",
					"multipleOf": 1.0
				},
				"gene_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"organism_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"internalComment": {
					"type": "string"
				},
				"schemaVersion": {
					"type": "number"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}