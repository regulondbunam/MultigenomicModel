{
	"validator": {
		"$jsonSchema": {
			"type": "object",
			"required": [
				"_id",
				"control_gc_phrases_id",
				"experimental_gc_phrases_id",
				"schemaVersion"
			],
			"properties": {
				"_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"control_gc_phrases_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"experimental_gc_phrases_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"note": {
					"type": "string"
				},
				"variable": {
					"type": "string"
				},
				"results": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "object",
						"required": ["_id", "identifier_id", "effect"],
						"properties": {
							"_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							},
							"identifier_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							},
							"identifier_type": {
								"type": "string"
							},
							"effect": {
								"type": "string"
							},
							"note": {
								"type": "string"
							}
						},
						"additionalProperties": false
					}
				},
				"schemaVersion": {
					"type": "number"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}