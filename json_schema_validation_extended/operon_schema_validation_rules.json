{
	"validator": {
		"$jsonSchema": {
			"type": "object",
			"required": [
				"_id",
				"name",
				"regulationPositions",
				"genes",
				"organism_id"
			],
			"properties": {
				"_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"genes": {
					"type": "array",
					"uniqueItems": true,
					"items": {
						"type": "object",
						"properties": {
							"gene_id": {
								"type": "string",
								"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
							},
							"leftEndPosition": {
								"type": "number",
								"multipleOf": 1.0
							},
							"rightEndPosition": {
								"type": "number",
								"multipleOf": 1.0
							}
						}

					}
				},
				"internalComment": {
					"type": "string"
				},
				"name": {
					"type": "string"
				},
				"organism_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"regulationPositions": {
					"type": "object",
					"required": [
						"leftEndPosition",
						"rightEndPosition"
					],
					"properties": {
						"leftEndPosition": {
							"type": "number",
							"multipleOf": 1.0
						},
						"rightEndPosition": {
							"type": "number",
							"multipleOf": 1.0
						}
					}
				},
				"strand": {
					"type": "string",
					"enum": ["forward", "reverse"]
				},
				"type": {
					"type": "string",
					"enum": ["simple", "complex"]
				},
				"schemaVersion": {
					"type": "number"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}