{
	"validator": {
		"$jsonSchema": {
			"type": "object",
			"required": [
				"_id",
				"organism_id"
			],
			"properties": {
				"_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"leftEndPosition": {
					"type": "number",
					"multipleOf": 1.0
				},
				"rightEndPosition": {
					"type": "number",
					"multipleOf": 1.0
				},
				"strand": {
					"type": "string",
					"enum": ["forward", "reverse"]
				},
				"sequence": {
					"type": "string",
					"pattern": "^[ATCGatcg]+$"
				},
				"type": {
					"type": "string"
				},
				"description": {
					"type": "string"
				},
				"score": {
					"type": "number"
				},
				"gene_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"organism_id": {
					"type": "string",
					"pattern": "^RDB[A-Z]{7}[0-9]{5}$"
				},
				"schemaVersion": {
					"type": "number"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}