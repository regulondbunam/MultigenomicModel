{
	"validator": {
		"$jsonSchema": {
			"bsonType": "object",
			"required": [
				"_id",
				"name",
				"organism_id",
				"schemaVersion"
			],
			"properties": {
				"_id": {
					"bsonType": "string"
				},
				"name": {
					"bsonType": "string"
				},
				"typeObject": {
					"bsonType": "array",
					"uniqueItems": true,
					"items": {
						"bsonType": "string"
					}
				},
				"code": {
					"bsonType": "string"
				},
				"note": {
					"bsonType": "string"
				},
				"internalComment": {
					"bsonType": "string"
				},
				"organism_id": {
					"bsonType": "string"
				},
				"schemaVersion": {
					"bsonType": "double"
				}
			},
			"additionalProperties": false
		}
	},
	"validationLevel": "strict",
	"validationAction": "error"
}